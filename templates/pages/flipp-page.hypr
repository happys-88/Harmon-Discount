{% extends "page" %}

{% block body-content %}
	{% block flipp-page-script %}
        <script src="https://circulars.harmondiscount.com/hosted_services/iframe.js"></script>
        <div id="flipp-container"></div>
        <script>
            // Flipp Hosted Circular
            var containerId = 'flipp-container';
            var circFrame = new wishabi.hostedservices.iframe.decorate(
                containerId,
                'harmonfacevalues',
                wishabi.hostedservices.iframe.Sizing.PAGE,
                {
                    minHeight: 500,
                    initialHeight: 500,
                    extraPadding: 0,
                    queryParameters: ''
                }
            );
        </script>
    {% endblock flipp-page-script %}  
{% endblock body-content %}